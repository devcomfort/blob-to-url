{
  "compilerOptions": {
    "target": "ES2020",
    "declaration": true,
    "declarationMap": false,
    "sourceMap": false,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": [
      "vitest/globals"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
}