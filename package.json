{
  "name": "blob-to-url",
  "description": "Simple Blob to URL code snippet. (ESM/CJS support)",
  "version": "0.0.15",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:clean": "npm run clean && npm run build",
    "build:cjs": "tsc --p ./cjs/tsconfig.json",
    "build:esm": "tsc --p ./esm/tsconfig.json",
    "clean": "npm run clean:cjs && npm run clean:esm",
    "clean:cjs": "tsc --build --clean ./cjs",
    "clean:esm": "tsc --build --clean ./esm"
  },
  "directories": {
    "lib": "./src"
  },
  "files": [
    "@types",
    "esm",
    "cjs",
    "src"
  ],
  "author": {
    "name": "devcomfort",
    "email": "kimdonghyun026@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devcomfort/blob-to-url.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "keywords": [
    "blob",
    "datauri",
    "base64",
    "file"
  ],
  "dependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "blob-polyfill": "^7.0.20220408",
    "jest": "^29.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
