	"version": "0.2.4",
	"type": "module",
	"types": "./dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"types": "./dist/esm/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"scripts": {
		"build": "rollup -c"
	},
	"files": ["dist"],
	"author": {
		"name": "devcomfort",
		"email": "im@devcomfort.me"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/devcomfort/blob-to-url.git"
	},
