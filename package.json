{
  "name": "blob-to-url",
  "version": "0.0.4",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs; npm run build:esm",
    "build:cjs": "tsc --p ./cjs/tsconfig.json",
    "build:esm": "tsc --p ./esm/tsconfig.json"
  },
  "author": {
    "name": "devcomfort",
    "email": "kimdonghyun026@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devcomfort/blob-to-url.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "keywords": [
    "blob",
    "datauri",
    "base64",
    "file"
  ],
  "dependencies": {
    "@types/node": "^18.11.18",
    "typescript": "^4.9.4"
  },
  "workspaces": {
    "packages": [
      "./cjs",
      "./esm"
    ]
  }
