{
  "name": "blob-to-url",
  "description": "Simple Blob to URL code snippet. (ESM/CJS support)",
  "packageManager": "yarn@3.2.3",
  "version": "0.0.8",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs; npm run build:esm",
    "build:cjs": "tsc --p ./cjs/tsconfig.json",
    "build:esm": "tsc --p ./esm/tsconfig.json"
  },
  "files": [
    "./esm/**/*",
    "./cjs/**/*"
  ],
  "author": {
    "name": "devcomfort",
    "email": "kimdonghyun026@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devcomfort/blob-to-url.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "keywords": [
    "blob",
    "datauri",
    "base64",
    "file"
  ],
  "dependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "blob-polyfill": "^7.0.20220408",
    "jest": "^29.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "workspaces": {
    "packages": [
      "./cjs",
      "./esm"
    ]
  }
}
