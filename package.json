{
  "name": "blob-to-url",
  "description": "Simple Blob to URL code snippet. (ESM/CJS support)",
  "version": "0.2.1",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build:clean": "npm run clean && npm run build",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --p tsconfig.cjs.json",
    "build:esm": "tsc --p tsconfig.esm.json",
    "clean": "npm run clean:cjs && npm run clean:esm",
    "clean:cjs": "npm exec rimraf dist/cjs",
    "clean:esm": "npm exec rimraf dist/esm"
  },
  "files": [
    "@types",
    "esm",
    "cjs",
    "src"
  ],
  "author": {
    "name": "devcomfort",
    "email": "kimdonghyun026@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devcomfort/blob-to-url.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "keywords": [
    "blob",
    "datauri",
    "base64",
    "file"
  ],
  "dependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "rimraf": "^5.0.1"
  }
}
